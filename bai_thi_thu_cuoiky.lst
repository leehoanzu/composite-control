.................... {      
....................       mpt1=key_nhan();        
....................       if (mpt1!=0xff)    
....................       {  delay_ms(1);   
....................          mpt1=key_nhan(); 
....................          do{mpt2=key_nhan(); delay_ms(30);} 
....................          while (mpt2==mpt1); 
....................       }              
....................       return(mpt1); 
.................... } 
